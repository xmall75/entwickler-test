    "seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "prettier": "prettier --write .",
    "prettier-check": "prettier --check .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
